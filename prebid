\"state\"\n                   Default value is the skinz leaderboard height\n          sublimeOffset : Change its value to add an offset to the sublimeLimit. \n    */\n    var sublimeOffset = Offset;\n    var sublimeLimit = 200;\n\n    \n  console.groupCollapsed(\'%c ScrollPageDesktop function log \', \'color:blue; background-color:lightblue;\');\n      console.log(\'Using ScrollPageDesktop function\');\n      console.log(\'sublimeLimit : \'+ sublimeLimit);\n\n      // Test if sublimeLimit exist AND sublimeLimit is positive\n      if (sublimeLimit !== null && typeof sublimeLimit !== \"undefined\" && sublimeLimit > 0) { \n       // Instructions that should be executed initially\n          sublimeElem.style.cssText = \"top:188px !important;\";\n          // sublimeScroll : variable that allow us to remove the event listener in case we have to clear Sublime elements\n       sublime.window.document.addEventListener(\'scroll\', sublimeScroll =  function () {\n        var sublimePageYOffset = sublime.window.pageYOffset;\n        if (sublimePageYOffset > sublimeLimit) {\n         // Instructions that should be executed when the sublimePageYOffset value is higher to sublimeLimit\n         sublimeElem.style.cssText = \"position:fixed !important; top:58px !important;\";\n        }\n        else {\n         // Instructions that should be executed when the sublimePageYOffset value is lower and equal to sublimeLimit\n         var sublimeTop = sublimeLimit - sublimePageYOffset;\n               sublimeElem.style.cssText = \"top:188px !important;\";\n        }\n       });\n   }\n   else {\n    // Test if sublimeLimit is negative\n    if (sublimeLimit < 0) {\n     console.error(\"The scroll limit (sublimeLimit) value is negative\");\n    }\n    else {\n     console.error(\"The scroll limit (sublimeLimit) value doesn\'t exist\");\n    }\n   }\n\n  console.groupEnd();\n }\n\n    function checkDivPresenceScrollDesktop(div, shift, callback){\n        myDiv = sublime.window.document.querySelector(div);\n        i = 0;\n        if (myDiv) {\n            callback(div, shift);\n        } else {\n            top.sublime.timer.setTimeout(function(){\n                if (i > 30) {\n                    top.sublime.timer.clearTimeout(function() {\n                        checkDivPresenceScrollDesktop(div, shift, callback);\n                    });\n                } else {\n                checkDivPresenceScrollDesktop(div, shift, callback);\n                }\n                i += 1;\n            }, 100);\n        }\n    }\n // The hook is to ensure sublimeAyadsHtml.offsetHeight is higher than 0 in HTML5 integration cases.\n sublime.hook.register(\'step.impression.sec1\', function () {\n  // Change the \'#sublimeElem\' to the CSS Selector of the element that should change following the scroll\n  // Change the \'0\' to an integer to add an offset to the scroll limit\n       checkDivPresenceScrollDesktop(\'#js-article-gsc-banner\', 0, ScrollPageDesktop);\n    });\n</script>\n\n<script>\nfunction init(){\n    /* Init variables */\n    var html = sublime.window.document.querySelector(\'html\'),\n        body = sublime.window.document.body,\n        isSafari = navigator.userAgent.indexOf(\'Safari\') != -1 && navigator.userAgent.indexOf(\'Chrome\') == -1,\n        isChrome = navigator.userAgent.indexOf(\'Chrome\') != -1,\n        isMac = navigator.appVersion.indexOf(\"Mac\") != -1,\n        isVideo = !sublime.isEmpty(sublime.creativeAd.videos),\n        bgfix = sublime.creativeAd.fixed,\n        format = \'\',\n        ayadsVideoLayer,\n        firstCall,\n        i = 0,\n        /* Edit variables */\n        maxHeightHeader = 191,\n        minHeightHeader = 58;\n    /**************** ADAPT MARGIN TO HTML AND LAUNCH FUNCTION FOR OTHERS ELEMENTS ****************/\n    function fixSkin(){\n     isChrome ? html.style.cssText = \'padding-top:\' + maxHeightHeader + \'px !important;\' : html.style.cssText = \'margin-top:\' + maxHeightHeader + \'px !important;\';\n   
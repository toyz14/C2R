round-position:center \' + newMarginTop + \'px !important;\';\n            }\n        }\n        /**************** CHECK IF ELEMENT IS PRESENT ****************/\n        function checkDivPresence(div, callback){\n            myDiv = sublime.window.document.querySelector(div);\n            if (myDiv) {\n                callback(myDiv);\n            } else {\n                top.sublime.timer.setTimeout(function(){\n                    if (i > 30) {\n                        top.sublime.timer.clearTimeout(function() {\n                            checkDivPresence();\n                        });\n                    } else {\n                        checkDivPresence(div, callback);\n                    }\n\n                    i += 1;\n                }, 100);\n            }\n        }\n        /**************** ADAPT MARGIN TO HEADER WITH A DYNAMIC HEIGHT (OPTION) ****************/\n    function getHeightDynamicHeader(headerDyn){\n        if (headerDyn.id === \"js-article-gsc-banner\") {\n          maxHeightHeader = headerDyn.offsetHeight + 191;\n          minHeightHeader = headerDyn.offsetHeight + 58;\n          var sublimeButton = top.document.querySelector(\"#js-article-gsc-close\");\n          sublimeButton.addEventListener(\"click\", function () {\n            maxHeightHeader = 191;\n            minHeightHeader = 58;\n            fixSkin();\n          });\n        }\n        else if (headerDyn.id === \"en-continu\") {\n          maxHeightHeader = headerDyn.offsetHeight + 200;\n        } else {\n          maxHeightHeader = headerDyn.offsetHeight + 191;\n        }\n        fixSkin();\n    }\n    \n        function sublimeAdaptInit () {\n      console.groupCollapsed(\'%c sublimeAdaptInit function log\', \'color:blue; background-color:lightblue;\');\n      top.document.addEventListener(\"scroll\", sublimeAdapt);\n      console.log(\"sublimeAdapt activated on scroll\");\n      console.groupEnd();\n    }\n    \n    function sublimeAdapt () {\n      var sublimeElem = top.document.querySelector(\'.lmd-banners\');  \n      var minHeightHeaderLast = minHeightHeader;\n      //console.groupCollapsed(\'%c sublimeAdapt function log\', \'color:blue; background-color:lightblue;\');\n      //console.log(sublimeElem);\n      \n      if(maxHeightHeader !== maxHeightHeaderLast) {\n     //console.log(\"maxHeightHeader : \"+ maxHeightHeader);\n      //console.log(\"maxHeightHeaderLast : \"+ maxHeightHeaderLast);\n            fixSkin();\n        }\n\n      if (sublimeElem) {\n          if (sublimeElem.classList.contains(\"lmd-u-block\") === true) {\n            var sublimeElem2 = top.document.querySelector(\"#js-article-gsc-banner\");\n            if (sublimeElem2) {\n            minHeightHeader = sublimeElem2.offsetHeight + sublimeElem.offsetHeight + 58;\n            } else {\n            minHeightHeader = sublimeElem.offsetHeight + 58;\n            }\n          } else {\n          minHeightHeader = 58;\n          }\n        if (minHeightHeader !== minHeightHeaderLast) {\n     //console.log(\"minHeightHeader : \"+ minHeightHeader);\n      //console.log(\"minHeightHeaderLast : \"+ minHeightHeaderLast);\n            fixSkin();\n        }\n      }\n      //console.groupEnd();\n    }\n    //checkDivPresence(\'.lmd-banners\', sublimeAdaptInit);\n    \n    function closeSubcribe() {\n      var sublimeElem = top.document.querySelector(\'.lmd-banners\');  \n      if (sublimeElem) {\n        var sublimeElemClose = top.document.querySelector(\'.lmd-banners .fcst-close\'); \n        if (sublimeElemClose) {\n        sublimeElemClose.addEventListener(\"click\", function () {\n           minHeightHeader = 58;\n           fixSkin();\n        });\n        }\n      }\n    }\n    \n     checkDivPresence(\'.lmd-banners\', closeSubcribe);\n        /**************** LAUNCH FUNCTION ****************/\n        /* USE THIS ONLY IF YOU HAVE TO FORCE THE BILLBOARD HEIGHT */\n        var sublimeBody = top.document.body;\n        var sublimePrev = top.document.querySelector(\"body > main\");\n        var sublimeSpaceTest = document.querySelector(\"#sublime-space\");\n        if (!sublimeSpaceTest) {\n        var sublimeSpace = top.document.createElement(\"div\");\n        sublimeSpace.id = \"sublime-space\";\n        \n        //sublimeBody.insertBefore(sublimeSpace, sublimePrev);\n       //sublime.display.addCss(\'#sublime-space {height: \' + sublimeAdTop + \'px !important;}\');\n       //sublime.display.addCss(\'#sublime-space {height: 0px !important;} body > main {transform: translateY(\'+sublimeAdTop+\'px) !important;}\');\n       }\n       sublime.display.addCss(\'body > main {transform: translateY(\'+sublimeAdTop+\'px) !important;}\');\n       \n        if (!isVideo){\n            sublime.hook.register(\"dom.ready\", function(){\n                firstCall = true; \n                fixSkin();\n            });\n        } else {\n            sublime.hook.register(\"step.videoContainerAdded\", function(){\n                ayadsVideo = sublime.document.querySelector(\"#ayads-video-container\");\n                ayadsVideoLayer = sublime.document.querySelector(\"#ayads-video-layer\");\n                firstCall = true;\n                fixSkin();\n            });\n        };\n        sublime.hook.register(\"event.cleanUp\", function(){\n            sublime.window.removeEventListener(\'scroll\', fixSkinOnScrollEvent);\n        });\n\n        /* USE THIS IF THE HEADER HAS NOT THE SAME HEIGHT ON DIFFERENT PAGES */\n        checkDivPresence(\'#en-continu\', getHeightDynamicHeader);\n         checkDivPresence(\'#js-article-gsc-banner\', getHeightDynamicHeader);\n    }\n    /**************** LAUNCH GLOBAL FUNCTION ****************/\n    init();\n</script>","use_cls_mobile":0,"mobile_css_cls":"","mobile_bottom_js":"","mobile_bottom_css":"","use_desktop_integration":1,"use_cls_tablet":1,"tablet_html_cls":"<style>\n    iframe[id^=\"ayads-pull-creative\"], .ayads-write, #banniere_haute, #banniere_haute iframe[height=\"1000\"] {display: none !im